{
    "version": 2,
    "functions": {
        "api/index.ts": {
            "includeFiles": "prisma/**"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index"
        }
    ],
    "env": {
        "PRISMA_CLI_BINARY_TARGETS": "native,rhel-openssl-1.0.x"
    }
}
